<?xml version="1.0" encoding="utf-8"?>
<com.caihui.zlzb_test.widget.MultiState2View xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_msv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.caihui.zlzb_test.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin">

        <TextView
            android:id="@+id/main_user"
            style="@style/common_tv_style" />

        <TextView
            android:id="@+id/main_at"
            style="@style/common_tv_style" />

        <TextView
            android:id="@+id/main_rt"
            style="@style/common_tv_style" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="login"
            android:text="Api  登录"
            android:textAllCaps="false" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="position"
            android:text="Api 已发布职位"
            android:textAllCaps="false" />
    </LinearLayout>

</com.caihui.zlzb_test.widget.MultiState2View>
